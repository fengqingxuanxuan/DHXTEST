<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    tools:context="com.tiantianle.activity.PasswordActivity">

    <include layout="@layout/title_red"></include>



    <com.allen.library.SuperTextView
        android:id="@+id/tv_login_password"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:sLeftTextMarginLeft="10dp"
        app:sLeftTextString="@string/Modify_login_password"
        app:sRightIconHeight="17dp"
        app:sRightIconRes="@mipmap/right"
        app:sRightIconWidth="17dp"
        app:sUseRipple="true" />

    <com.allen.library.SuperTextView
        android:id="@+id/tv_trading_password"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:sLeftTextMarginLeft="10dp"
        app:sLeftTextString="@string/Modify_trading_password"
        app:sRightIconHeight="17dp"
        app:sRightIconRes="@mipmap/right"
        app:sRightIconWidth="17dp"
        app:sUseRipple="true" />

</LinearLayout>