<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 圆角 -->
    <corners
        android:radius="5dp"
        android:topLeftRadius="4dp"
        android:topRightRadius="4dp"
        android:bottomLeftRadius="4dp"
        android:bottomRightRadius="4dp"/><!-- 设置圆角半径 -->


    <!-- 间隔 -->
    <padding
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp"/><!-- 各方向的间隔 -->

    <!-- 描边 -->
    <stroke
        android:width="1dp"
        android:color="@color/clolrTextNorenl" />


    <!-- 填充 -->
    <solid
        android:color="@color/flowlayout_unchecked_color"/><!-- 填充的颜色 -->


</shape>